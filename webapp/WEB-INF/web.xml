<?xml version="1.0"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN" "http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">
<web-app>

 <display-name>Address Geocoder Web Demo</display-name>
 <description>Address Geocoder Web Demo</description>

 <servlet>
  <servlet-name>GeocoderServlet</servlet-name>
  <servlet-class>net.sourceforge.jgeocoder.JGeocoderServlet</servlet-class>
 </servlet>
 <servlet-mapping>
  <servlet-name>GeocoderServlet</servlet-name>
  <url-pattern>/geocoder/*</url-pattern>
 </servlet-mapping>

</web-app>